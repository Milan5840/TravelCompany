@model TravelCompany.Web.Models.ExpensesViewModel

@{
    ViewData["Title"] = "AddExpense";
}

<h2>AddExpense</h2>

<h4>AddExpense</h4>
<form asp-action="AddExpense" enctype="multipart/form-data">
    <div asp-validation-summary="ModelOnly" class="form-group">
        <input type="hidden" asp-for="Travelid" />
        
        <partial name="_Expense" />

        <div class="form-group">
            <input type="submit" value="Create" class="btn btn-default" />
            <a asp-action="Details" asp-route-id="@Model.Travelid" class="btn btn-info">Back to List</a>
        </div>
        </div>
</form>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

